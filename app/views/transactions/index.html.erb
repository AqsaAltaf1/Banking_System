<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">Ammount</th>
      <th scope="col">Sender ID</th>
      <th scope="col">Reciever ID</th>
    </tr>
  </thead>
  <tbody>
    <% @transactions.each do |t| %>
      <tr>
        <td scope="row"><%= t.id %></td>
        <td><%= t.amount %></td>
        <td><%= t.account_id %></td> 
        <td><%=  t.reciever_id %></td> 
      </tr>
    <% end %>
  </tbody>
</table>